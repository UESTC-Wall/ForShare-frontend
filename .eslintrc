{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "linebreak-style": [2, "windows"],
        "react/no-multi-comp": 0,
        "quotes": 0,
        "no-alert": 0,
        "no-console": 0,
        "react/sort-comp": 0,
        "comma-dangle": 0,
        "jsx-a11y/href-no-hash": 0,
        "radix": 0,
        "react/no-danger": 0,
        "jsx-a11y/label-has-for": 0,
        "import/prefer-default-export": 0,
        "no-plusplus": 0,
        "no-param-reassign": 1,
        "no-useless-escape": 1,
    },
    "env": {
        "browser": true,
        "node": true
    }
} 